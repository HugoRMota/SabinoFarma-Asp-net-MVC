    <script>
        //captura a url, excluindo o domínio principal
        var url = window.location.pathname;
        //captura o elemento "a" que aponta para essa url
        var ativo = $("#stuck_container li a[href='" + url + "']");
        //remove a classe "active" de todos os menus
        $("#stuck_container li a").parent().removeClass("active");
        //adiciona a classe "active" ao menu com bref=url
        $(ativo).parent().addClass("active");
    </script>
